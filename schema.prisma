// database
datasource db {
  provider = "sqlite"
  url      = "file:./database.db"
}

// generator
generator client {
  provider             = "prisma-client-py"
  recursive_type_depth = 5
}

model Command_MDL {
  id String @id @default(uuid())
  userId BigInt
  name String
  response String
  enabled Boolean @default(true)
  guildId BigInt

  @@unique([name, guildId])
}